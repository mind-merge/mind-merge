{
  "author": "Alex Mincu @mincua",
  "bin": {
    "dev-copilot": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chokidar": "^3.6.0",
    "gray-matter": "^4.0.3",
    "liquidjs": "^10.10.0",
    "reflect-metadata": "^0.2.1",
    "typedi": "^0.10.0"
  },
  "description": "Dev Copilot that will let you build an army of agents designed for your project and code then work with them",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mincua/dev-copilot",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "dev-copilot",
  "oclif": {
    "bin": "dev-copilot",
    "dirname": "dev-copilot",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "mincua/dev-copilot",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/mincua/dev-copilot/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
