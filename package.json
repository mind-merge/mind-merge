{
  "author": "Alexandru Mincu",
  "bin": {
    "mm": "./bin/run.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.7.1",
    "@mind-merge-ai/base-agents": "^1.0.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/mime": "^3.0.4",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "gpt-tokenizer": "^2.1.2",
    "gray-matter": "^4.0.3",
    "groq-sdk": "^0.3.3",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.10.0",
    "mime": "^4.0.1",
    "openai": "^4.28.0",
    "reflect-metadata": "^0.2.1",
    "typedi": "^0.10.0"
  },
  "description": "Mind-Merge is a copilot that will let you build an army of agents designed for your project and code then work with them",
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.2",
    "@cucumber/messages": "^24.1.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4.3.14",
    "@types/cucumber": "^7.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^5.1.0",
    "cucumber-tsflow": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mem-fs": "^4.1.0",
    "mem-fs-editor": "^11.0.0",
    "mocha": "^10",
    "nock": "^14.0.0-beta.5",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mincua/mm",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "name": "@mind-merge-ai/mind-merge",
  "oclif": {
    "bin": "mm",
    "dirname": "mind-merge",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "mind-merge/mind-merge",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test-feature": "cucumber-js",
    "test-cucumber": "./node_modules/.bin/cucumber-js -p default",
    "version": "oclif readme && git add README.md",
    "start": "node ./bin/dev.js start"
  },
  "version": "0.0.1",
  "bugs": "https://github.com/mincua/mind-merge/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
